        "grunt-jscs": "^3.0.0",
        "grunt-karma": "^2.0.0",
        "grunt-protractor-runner": "^3.2.0",
        "http-server": "^0.9.0",
        "jasmine-core": "^2.4.1",
        "jasmine-reporters": "^2.1.1",
        "karma": "^0.13.22",
        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.0.0",
        "karma-firefox-launcher": "^1.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-junit-reporter": "^1.0.0",
        "karma-phantomjs-launcher": "^1.0.0",
